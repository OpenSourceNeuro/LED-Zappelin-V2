	background-color: rgb(0, 43, 54);
}


/* --- Bottom Frame --- */

#BottomMainFrame{
	background-color: rgb(0, 30, 38);
}


/* --- Menu Frame --- */

#Menu_Frame{
	background-color: rgb(0, 30, 38);
}
#Menu_Frame QPushButton{
	text-align: left;
	padding: 10px 5px;
	border-top-left-radius:15px;
